// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TestResult {
  id              String   @id @default(cuid())
  isp             String
  city            String
  downloadSpeed   Float
  uploadSpeed     Float
  latency         Int
  jitter          Int
  throttlingRatio Float?
  createdAt       DateTime @default(now())
}